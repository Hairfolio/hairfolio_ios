{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "require": true,
    "window": true,
    "navigator": true,
    "console": true,
    "setImmediate": true,
    "setInterval": true,
    "clearInterval": true,
    "setTimeout": true,
    "requestAnimationFrame": true,
    "__DEV__": true,
    "process": true
  },
  "rules": {
    "brace-style": [1, "1tbs", {"allowSingleLine": true}],
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": [1, "last"],
    "consistent-this": [1, "self"],
    "eol-last": 1,
    "guard-for-in": 0,
    "indent": [2, 2, {
      "SwitchCase": 1
    }],
    "key-spacing": 0,
    "new-cap": [1, {"capIsNew": false, "newIsCap": true}],
    "new-parens": 0,
    "no-bitwise": 0,
    "no-cond-assign": 1,
    "no-console": 0,
    "no-constant-condition": 1,
    "no-delete-var": 2,
    "no-debugger": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-eval": 1,
    "no-ex-assign": 1,
    "no-extend-native": 1,
    "no-floating-decimal": 1,
    "no-func-assign": 1,
    "no-implied-eval": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 1,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-wrappers": 2,
    "no-obj-calls": 1,
    "no-octal": 1,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow": 0,
    "no-spaced-func": 1,
    "no-sparse-arrays": 1,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-undefined": 2,
    "strict": 0,
    "no-undef-init": 1,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-use-before-define": [1, "nofunc"],
    "no-warning-comments": [0, {"terms": ["todo", "fixme"]}],
    "no-extra-parens": 0,
    "quotes": [1, "single", "avoid-escape"],
    "quote-props": [0, "as-needed"],
    "radix": 1,
    "semi-spacing": [1, {"before": false, "after": true}],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "space-infix-ops": 1,
    "curly-spacing": 0,
    "space-in-parens": [1, "never"],
    "space-unary-ops": [1, {"words": true, "nonwords": false}],
    "use-isnan": 1,
    "valid-typeof": 1,
    "valid-jsdoc": [2, {"requireReturn": false}],
    "vars-on-top": 0,
    "wrap-iife": [1, "inside"],
    "yoda": 0,
    "handle-callback-err": [1, "^(err|error)$"],
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,
    "react/jsx-boolean-value": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-unknown-property": 1,
    "react/react-in-jsx-scope": 1,
    "react/sort-comp": [1, {
      "order": [
        "state",
        "lifecycle",
        "everything-else",
        "render"
      ]
    }],
    "react/wrap-multilines": 1
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  }
}
